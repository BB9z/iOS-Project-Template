<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
	<key>DEFAULT</key>
	<dict>
		<key>Base</key>
		<string>http://example.com</string>
		<key>Path Prefix</key>
		<string>api/index?c=</string>
		<key>Method</key>
		<string>GET</string>
		<key>Authorization</key>
		<true/>
		<key>Cache Policy</key>
		<integer>0</integer>
		<key>Offline Policy</key>
		<integer>0</integer>
		<key>Expire</key>
		<string>60</string>
	</dict>
	<key>@ Account</key>
	<dict>
		<key>Login</key>
		<dict>
			<key>Method</key>
			<string>POST</string>
			<key>Path</key>
			<string>user/login</string>
			<key>Authorization</key>
			<false/>
			<key>Response Class</key>
			<string>AccountEntity</string>
			<key>Response Type</key>
			<integer>2</integer>
		</dict>
		<key>AcoountInfo</key>
		<dict>
			<key>Path</key>
			<string>user/info</string>
			<key>Response Class</key>
			<string>AccountEntity</string>
			<key>Response Type</key>
			<integer>2</integer>
		</dict>
	</dict>
	<key>@ 支持分组</key>
	<dict>
		<key>User Reset Password</key>
		<dict>
			<key>Path</key>
			<string>user/reset</string>
			<key>Authorization</key>
			<false/>
			<key>Notes</key>
			<string>密码重置</string>
		</dict>
		<key>User Change Password</key>
		<dict>
			<key>Path</key>
			<string>user/password</string>
			<key>Notes</key>
			<string>修改密码</string>
		</dict>
	</dict>
</dict>
</plist>
